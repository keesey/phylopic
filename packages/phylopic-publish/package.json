{
    "dependencies": {
        "@aws-sdk/client-lambda": "^3.52.0",
        "@aws-sdk/client-s3": "^3.52.0",
        "@aws-sdk/client-ssm": "^3.52.0",
        "axios": "^0.26.0",
        "concurrently": "^7.0.0",
        "dotenv": "^16.0.0",
        "metadata-scraper": "^0.2.49",
        "node-fetch": "^2.3.0",
        "normalize-diacritics": "^2.14.0",
        "parse-nomen": "^2.1.2",
        "path": "^0.12.7",
        "pg": "^8.7.3",
        "phylopic-api-models": "2.0.0-alpha.1",
        "phylopic-source-models": "2.0.0-alpha.1",
        "phylopic-utils": "2.0.0-alpha.1",
        "probe-image-size": "^7.2.3",
        "raw-body": "^2.4.2",
        "readline": "^1.3.0",
        "simple-digraph": "^1.0.1",
        "slugify": "^1.6.5",
        "stream": "^0.0.2",
        "ts-node": "^10.5.0",
        "typescript": "^4.5.5",
        "uuid": "^8.3.2"
    },
    "devDependencies": {
        "@types/node": "^16.11.13",
        "@types/node-fetch": "^2.1.4",
        "@types/pg": "^8.6.4",
        "@types/probe-image-size": "^7.0.1",
        "@types/uuid": "^8.3.4",
        "@typescript-eslint/eslint-plugin": "^5.11.0",
        "@typescript-eslint/parser": "^5.11.0",
        "eslint": "^8.9.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-import-resolver-alias": "^1.1.2",
        "eslint-plugin-import": "^2.25.4",
        "prettier": "^2.5.1"
    },
    "name": "phylopic-publisher",
    "private": true,
    "resolutions": {
        "@aws-sdk/signature-v4-crt": "^3.31.0",
        "@swc/core": ">=1.2.50",
        "@swc/wasm": ">=1.2.50",
        "bufferutil": "^4.0.1",
        "encoding": "^0.1.0",
        "pg-native": ">=2.0.0",
        "utf-8-validate": "^5.0.2"
    },
    "scripts": {
        "build": "ts-node ./src/build.ts 2> build-error.log",
        "cli": "ts-node ./src/cli.ts",
        "download": "concurrently \"yarn download:images\" \"yarn download:source\"",
        "download:images": "aws s3 sync --delete s3://images.phylopic.org/ ./.s3/images.phylopic.org",
        "download:source": "aws s3 sync --delete s3://source.phylopic.org/ ./.s3/source.phylopic.org",
        "format": "prettier -w . && eslint --ext .js,.ts --fix .",
        "heal": "ts-node ./src/heal.ts",
        "lint": "eslint --ext .js,.ts .",
        "make": "yarn download && yarn process && concurrently \"yarn build\" \"yarn upload\" && yarn release && yarn sync",
        "process": "./process.sh 2> process-error.log",
        "release": "ts-node ./src/release.ts 2> release-error.log",
        "sync": "yarn sync:images",
        "sync:images": "aws s3 sync --acl public-read --delete ./.s3/images.phylopic.org s3://images.phylopic.org/",
        "upload": "yarn upload:images",
        "upload:images": "aws s3 sync --acl public-read ./.s3/images.phylopic.org s3://images.phylopic.org/"
    },
    "version": "2.0.0-alpha.1"
}
