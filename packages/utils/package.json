    },
    "dependencies": {
        "email-validator": "^2.0.4",
        "parse-nomen": "^2.1.2",
        "uuid": "^8.3.2"
    },
    "description": "Common code for *PhyloPic* projects.",
    "devDependencies": {
        "eslint": "^8.6.0",
        "mocha": "^9.1.4",
        "prettier": "^2.5.1",
        "typescript": "^4.6.4",
        "yarn": "^1.22.18"
    },
    "exports": {
        ".": {
            "import": "./dist/esm/index.js",
            "require": "./dist/index.js",
            "types": "./dist/index.d.ts"
        }
    },
    "files": [
        "dist"
    ],
    "homepage": "https://github.com/keesey/phylopic/packages/utils#readme",
    "keywords": [
        "phylopic"
    ],
    "license": "MIT",
    "main": "dist/index.js",
    "name": "@phylopic/utils",
    "publishConfig": {
        "access": "public"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/keesey/phylopic.git"
    },
    "scripts": {
        "build": "tsc -p tsconfig.cjs.json && tsc -p tsconfig.esm.json && echo >dist/esm/package.json '{\"type\":\"module\"}'",
        "format": "prettier -w . && yarn lint --fix",
        "lint": "eslint --ext .js,.ts .",
        "postversion": "git push && git push --tags",
        "prebuild": "rm -rf dist",
        "prepare": "yarn build",
        "prepublishOnly": "yarn test && yarn lint",
        "preversion": "yarn format",
        "test": "mocha -r ts-node/register \"./src/**/*.spec.ts\"",
        "version": "git add -A src"
    },
    "type": "module",
    "types": "dist/index.d.ts",
    "version": "2.0.0-alpha"
}
