{
    "author": {
        "email": "keesey+phylopic@gmail.com",
        "name": "T. Michael Keesey"
    },
    "bugs": {
        "url": "https://github.com/keesey/phylopic/issues"
    },
    "dependencies": {
        "email-validator": "^2.0.4",
        "parse-nomen": "^2.1.3",
        "uuid": "^9.0.1"
    },
    "description": "Common code for *PhyloPic* projects.",
    "devDependencies": {
        "@phylopic/tsconfig": "*",
        "@types/chai": "^4.3.14",
        "@types/uuid": "^9.0.8",
        "eslint": "^8.57.0",
        "eslint-config-phylopic": "*",
        "prettier": "^3.2.5",
        "tsup": "^8.0.2",
        "typescript": "^5.4.3",
        "vitest": "^1.4.0",
        "yarn": "^1.22.22"
    },
    "exports": {
        "import": "./dist/index.mjs",
        "require": "./dist/index.cjs",
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
    },
    "files": [
        "dist/**"
    ],
    "homepage": "https://github.com/keesey/phylopic/packages/utils#readme",
    "keywords": [
        "phylopic"
    ],
    "license": "MIT",
    "main": "./dist/index.mjs",
    "module": "./dist/index.js",
    "name": "@phylopic/utils",
    "publishConfig": {
        "access": "public"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/keesey/phylopic.git"
    },
    "scripts": {
        "build": "tsup",
        "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
        "dev": "tsup --watch",
        "lint": "TIMING=1 eslint src/**/*.ts* --fix",
        "test": "vitest --watch=false"
    },
    "sideEffects": false,
    "tsup": {
        "clean": true,
        "dts": true,
        "entry": [
            "src/index.ts"
        ],
        "format": [
            "cjs",
            "esm"
        ],
        "sourcemap": true,
        "splitting": true,
        "target": "es2022"
    },
    "types": "dist/index.d.ts",
    "version": "1.0.1"
}
