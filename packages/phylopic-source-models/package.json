{
    "author": "T. Michael Keesey <keesey@gmail.com>",
    "dependencies": {
        "email-validator": "^2.0.4",
        "parse-nomen": "^2.1.2",
        "uuid": "^8.3.2"
    },
    "description": "Types, validation, and normalization for PhyloPic model objects.",
    "devDependencies": {
        "@types/chai": "^4.3.0",
        "@types/mocha": "^9.0.0",
        "@types/uuid": "^8.3.4",
        "@typescript-eslint/eslint-plugin": "^5.10.1",
        "@typescript-eslint/parser": "^5.10.1",
        "chai": "^4.3.4",
        "eslint": "^8.6.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-import": "^2.25.4",
        "mocha": "^9.1.4",
        "prettier": "^2.5.1",
        "ts-node": "^10.4.0",
        "typescript": "^4.5.5",
        "yarn": "^1.22.18"
    },
    "files": [
        "dist/**/*"
    ],
    "license": "MIT",
    "main": "dist/index.js",
    "name": "phylopic-source-models",
    "resolutions": {
        "@types/node": "*"
    },
    "scripts": {
        "build": "rm -rf dist && mkdir dist && mkdir dist/models && cp src/models/*.d.ts dist/models && yarn compile",
        "compile": "tsc",
        "format": "prettier -w . && yarn lint --fix",
        "lint": "eslint --ext .js,.ts .",
        "postversion": "git push && git push --tags",
        "prepare": "yarn build",
        "prepublishOnly": "yarn test && yarn lint",
        "preversion": "yarn format",
        "test": "mocha -r ts-node/register \"./src/**/*.spec.ts\"",
        "version": "git add -A src"
    },
    "typings": "dist/index.d.ts",
    "version": "2.0.0-alpha.1"
}