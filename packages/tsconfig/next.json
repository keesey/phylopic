{
    "$schema": "https://json.schemastore.org/tsconfig",
    "compilerOptions": {
        "allowJs": true,
        "incremental": true,
        "isolatedModules": true,
        "jsx": "preserve",
        "lib": ["DOM", "DOM.Iterable", "ES2022"],
        "module": "ES2022",
        "noEmit": true,
        "outDir": "dist",
        "resolveJsonModule": true,
        "skipLibCheck": true
    },
    "display": "Next.js",
    "extends": "./base.json"
}
