{
    "author": {
        "email": "keesey+phylopic@gmail.com",
        "name": "T. Michael Keesey"
    },
    "bugs": {
        "url": "https://github.com/keesey/phylopic/issues"
    },
    "description": "Tools for using the *PhyloPic* API.",
    "devDependencies": {
        "@phylopic/api-models": "*",
        "@phylopic/utils": "*",
        "@phylopic/tsconfig": "*",
        "@types/react": "^18.0.12",
        "cross-fetch": "^3.1.5",
        "eslint-config-phylopic": "*",
        "react": "^18.1.0",
        "swr": "^1.3.0",
        "typescript": "^4.6.4"
    },
    "exports": {
        "import": "./dist/index.mjs",
        "require": "./dist/index.cjs",
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
    },
    "files": [
        "dist/**"
    ],
    "homepage": "https://github.com/keesey/phylopic/packages/utils-api#readme",
    "keywords": [
        "API",
        "PhyloPic"
    ],
    "license": "MIT",
    "main": "./dist/index.js",
    "mocha": {
        "extension": [
            "ts"
        ],
        "loader": "ts-node/esm",
        "spec": "src/**/*.spec.ts"
    },
    "module": "./dist/index.mjs",
    "name": "@phylopic/utils-api",
    "publishConfig": {
        "access": "public"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/keesey/phylopic.git"
    },
    "resolutions": {
        "@types/node": "*"
    },
    "scripts": {
        "build": "tsup src/index.ts --format esm,cjs --dts",
        "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
        "dev": "tsup src/index.ts --format esm,cjs --watch --dts",
        "lint": "TIMING=1 eslint src/**/*.ts* --fix",
        "test": "mocha"
    },
    "sideEffects": false,
    "types": "./dist/index.d.ts",
    "version": "0.0.0"
}
