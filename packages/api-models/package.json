{
    "author": {
        "email": "keesey+phylopic@gmail.com",
        "name": "T. Michael Keesey"
    },
    "bugs": {
        "url": "https://github.com/keesey/phylopic/issues"
    },
    "dependencies": {
        "@phylopic/utils": "file:../utils",
        "normalize-diacritics": "^3.0.8"
    },
    "description": "Type declarations and validation for PhyloPic API value objects.",
    "devDependencies": {
        "eslint": "^8.6.0",
        "mocha": "^9.1.4",
        "prettier": "^2.5.1",
        "typescript": "^4.6.4",
        "yarn": "^1.22.18"
    },
    "exports": {
        ".": {
            "import": "./dist/esm/index.js",
            "require": "./dist/index.js",
            "types": "./dist/index.d.ts"
        }
    },
    "files": [
        "dist"
    ],
    "homepage": "https://github.com/keesey/phylopic/packages/api-models#readme",
    "license": "MIT",
    "main": "dist/index.js",
    "name": "@phylopic/api-models",
    "publishConfig": {
        "access": "public"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/keesey/phylopic.git"
    },
    "resolutions": {
        "@types/node": "*"
    },
    "scripts": {
        "build": "tsc -p tsconfig.cjs.json && tsc -p tsconfig.esm.json && echo >dist/esm/package.json '{\"type\":\"module\"}'",
        "format": "prettier -w . && eslint --ext .js,.ts --fix .",
        "lint": "eslint --ext .js,.ts .",
        "postversion": "git push && git push --tags",
        "prebuild": "rm -rf dist",
        "prepare": "yarn build",
        "prepublishOnly": "yarn test && yarn lint",
        "preversion": "yarn lint && yarn test",
        "test": "mocha -r ts-node/register \"./src/**/*.spec.ts\"",
        "version": "git add -A src"
    },
    "type": "module",
    "types": "dist/index.d.ts",
    "version": "2.0.0-alpha"
}
